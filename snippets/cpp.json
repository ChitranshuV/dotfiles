            "    base %= MOD;",
            "    int result{1};",
            "    while (exp > 0) {",
            "        if (exp & 1)",
            "            result = ((ll)result * base) % MOD;",
            "        base = ((ll)base * base) % MOD;",
            "        exp >>= 1;",
            "    }",
            "    return result;",
            "}"
